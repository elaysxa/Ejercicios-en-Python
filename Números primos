# Mostrar los números primos del 1 al 50
for numero in range(1, 51):
    if numero > 1:  # Los números menores o iguales a 1 no son primos
        es_primo = True
        for i in range(2, numero):
            if numero % i == 0:
                es_primo = False
                break
        if es_primo:J
            print(numero)
